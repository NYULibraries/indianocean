<!doctype html>
<html>
<head>{{> head }}</head>
<body data-app="{{ appUrl }}" data-appRoot="{{ appRoot }}" class="{{ bodyClass }}">
  {{> header }}
  <main class="main container-fluid" role="main">
     {{#title}}
      <h2  class="page-title">{{{html}}}</h2>
      {{/title}}
    {{#content}}
  
      {{#main}}
      <div class="maintext {{class}}" >{{{html}}}</div>
      {{/main}}
  
    {{/content}}
  </main>
  {{> footer }}
  {{{ closure }}}  
</body>
</html>