---
import Logo from "../../Assets/Logo.astro";
import FooterPartnerItems from "./FooterPartnerItems.astro";

const FooterLogo = JSON.parse(import.meta.env.PUBLIC_FOOTERLOGO);
const partners = JSON.parse(import.meta.env.PUBLIC_FOOTERPARTNERS)

---

<footer class="footer-main container-fluid" role="contentinfo">
    <Logo href={FooterLogo.href} src={FooterLogo.src} alt={FooterLogo.alt} />
  <div class="partner-wrapper">
    {
      partners.map((item) => (
        <FooterPartnerItems id={item.id} url={item.url} label={item.label}/>
      ))
    }
  </div>
</footer>
